{
  "version": "1.0.0-rc2-final",
  "description": "SQLite implementation of the System.Data.Common provider model.",
  "buildOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true,
    "embed": {
      "include": [
        "Properties/Microsoft.Data.Sqlite.rd.xml",
        "Strings.resx"
      ]
    }
  },
  "frameworks": {
    "net451": {
      "frameworkAssemblies": {
        "System.Data": ""
      }
    },
    "netstandard1.3": {
      "dependencies": {
        "System.AppContext": "4.1.0-rc2-24027",
        "System.Data.Common": "4.0.1-rc2-24027",
        "System.Diagnostics.Debug": "4.0.11-rc2-24027",
        "System.Linq": "4.1.0-rc2-24027",
        "System.Reflection.Extensions": "4.0.1-rc2-24027",
        "System.Resources.ResourceManager": "4.0.1-rc2-24027",
        "System.Runtime.Extensions": "4.1.0-rc2-24027",
        "System.Runtime.InteropServices": "4.1.0-rc2-24027",
        "System.Text.Encoding": "4.0.11-rc2-24027"
      }
    },
    "netcore50": {
      "dependencies": {
        "Microsoft.NETCore.Platforms": {
          "type": "build",
          "version": "1.0.1-rc2-24027"
        },
        "System.AppContext": "4.1.0-rc2-24027",
        "System.Data.Common": "4.0.1-rc2-24027",
        "System.Diagnostics.Debug": "4.0.11-rc2-24027",
        "System.Linq": "4.1.0-rc2-24027",
        "System.Reflection.Extensions": "4.0.1-rc2-24027",
        "System.Resources.ResourceManager": "4.0.1-rc2-24027",
        "System.Runtime.Extensions": "4.1.0-rc2-24027",
        "System.Runtime.InteropServices": "4.1.0-rc2-24027",
        "System.Text.Encoding": "4.0.11-rc2-24027",
        "System.Threading": {
          "type": "build",
          "version": "4.0.11-rc2-24027"
        },
        "System.Text.RegularExpressions": {
          "type": "build",
          "version": "4.0.12-rc2-24027"
        }
      }
    }
  },
  "packInclude": {
    "build/": "build/**",
    "runtimes/": "runtimes/**"
  }
}